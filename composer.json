{
  "name": "dozer111/array_sorter",
  "description": "This is a default structure for PHP projects.",
  "keywords": ["project", "template", "structure", "skeleton", "phpunit"],
  "type": "project",
  "license": "proprietary",
  "authors": [
    {
      "name": "Khonko alex",
      "email": "dest2r4@gmail.com"

    }
  ],
  "minimum-stability": "stable",
  "require": {
    "php": ">=7.1"
  },
  "require-dev": {
    "phpunit/phpunit": "^8.0",
    "friendsofphp/php-cs-fixer": "^2.14"
  },
  "autoload": {
    "psr-4": {
      "dozer111\\array_sorter\\":"src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "App\\Test\\": "tests/"
    }
  },
  "scripts": {
    "cs-check": "php-cs-fixer fix --allow-risky=yes --diff --ansi --dry-run",
    "cs-fix": "php-cs-fixer fix --allow-risky=yes",
    "tests": "phpunit --colors=always"
  },
  "config": {
    "sort-packages": true
  }
}
